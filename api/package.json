{
  "name": "sims-api",
  "version": "0.0.0",
  "description": "API for SIMS Web App",
  "license": "Apache-2.0",
  "main": "app",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/biohubbc.git"
  },
  "scripts": {
    "start": "ts-node src/app",
    "clean": "gulp clean",
    "build": "gulp build",
    "start:reload": "./node_modules/.bin/nodemon src/app.ts --exec ts-node",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "coverage": "nyc mocha",
    "lint": "eslint . --ignore-pattern 'node_modules' --ext .ts",
    "lint:fix": "eslint . --fix --ignore-pattern 'node_modules' --ext .ts",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "adm-zip": "~0.5.5",
    "ajv": "~8.6.3",
    "aws-sdk": "~2.814.0",
    "axios": "~0.23.0",
    "db-migrate": "~0.11.11",
    "db-migrate-pg": "~1.2.2",
    "xlsx": "~0.17.3",
    "clamdjs": "~1.0.2",
    "express": "~4.17.1",
    "express-openapi": "~9.3.0",
    "jsonpath": "~1.1.1",
    "jsonwebtoken": "~8.5.1",
    "jwks-rsa": "~2.0.5",
    "knex": "~0.21.4",
    "lodash": "~4.17.21",
    "mime": "~2.5.2",
    "moment": "~2.29.1",
    "multer": "~1.4.3",
    "pg": "~8.7.1",
    "qs": "~6.10.1",
    "sql-template-strings": "~2.2.2",
    "typescript": "~3.9.4",
    "uuid": "~8.3.2",
    "winston": "~3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "~1.0.1",
    "@types/adm-zip": "~0.4.34",
    "@types/chai": "~4.2.22",
    "@types/express": "~4.17.13",
    "@types/geojson": "~7946.0.8",
    "@types/gulp": "~4.0.9",
    "@types/jsonpath": "~0.2.0",
    "@types/jsonwebtoken": "~8.5.5",
    "@types/lodash": "~4.14.176",
    "@types/mime": "~2.0.3",
    "@types/mocha": "~9.0.0",
    "@types/multer": "~1.4.7",
    "@types/node": "~14.14.31",
    "@types/pg": "~8.6.1",
    "@types/sinon": "~10.0.4",
    "@types/sinon-chai": "~3.2.5",
    "@types/uuid": "~8.3.1",
    "@types/yamljs": "~0.2.31",
    "@typescript-eslint/eslint-plugin": "~3.7.1",
    "@typescript-eslint/parser": "~3.7.1",
    "chai": "~4.3.4",
    "del": "~6.0.0",
    "eslint": "~7.5.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-prettier": "~3.1.4",
    "gulp": "~4.0.2",
    "gulp-typescript": "~5.0.1",
    "mocha": "~8.4.0",
    "nodemon": "~2.0.14",
    "npm-run-all": "~4.1.5",
    "nyc": "~15.1.0",
    "prettier": "~2.2.1",
    "sinon": "~11.1.2",
    "sinon-chai": "~3.7.0",
    "ts-mocha": "~8.0.0",
    "ts-node": "~10.4.0"
  }
}
